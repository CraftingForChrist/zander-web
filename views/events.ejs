<% include partials/header.ejs %>
<% include partials/navigation.ejs %>

<div class="container">
  <div class="jumbotron jumbotron-image-header my-4 home-jumbo-header" id="video-container">
    <div class="container text-white">
      <h1 class="display-3"><i class="fas fa-calendar"></i> Events</h1>
    </div>
  </div>

  <% if (!objdata[0]) { %>
    <% include partials/micros/noupcomingevents.ejs %>
  <% } else { %>
    <% objdata.forEach(function (data) { %>
      <div class="card">
        <div class="card-body">
          <div class="row">
            <div class="col-sm-">
              <img src="<%= data.eventicon %>" width="250px" height="250px">
            </div>
            <div class="col">
              <a href="#<%= data.eventtitle %>" style="color:black; text-decoration:none;"><h3><%= data.eventtitle %> <a href="#" style="color:black; font-size:14pt;"><i class="fas fa-link"></i></a></h3></a>
              <h6><%= data.eventdate %> @ <%= data.eventtime %></h6>
              <div class="">
                <p><%= data.eventinformation %></p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <br>
    <% }) %>
  <% } %>

</div>

<% include partials/footer.ejs %>
